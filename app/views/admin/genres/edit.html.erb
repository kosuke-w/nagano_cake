<h1>ジャンル編集</h1>

<%= form_with model: @genre, url: admin_genre_path(@genre.id), local:true do |f| %>
    <div class="form-group">
      <label>ジャンル名</label>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.submit '変更を保存', class: "btn btn-green" %>
    </div>
<% end %>